    "keywords": ["laravel", "Blade", "HTML"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Gomez",
            "email": "cristiangomeze@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
